\n}\r\n\r\n@mixin normal-2() {\r\n    font-family: 'Barlow', sans-serif;\r\n    font-size: 24pt;\r\n    font-weight: 400;\r\n    text-align: left;\r\n}\r\n\r\n\r\n// FILTRO\r\n@mixin filtro() {\r\n    width: 100%;\r\n    height: 100%;\r\n    background: rgba(0, 0, 0, 0.4);\r\n    display: inherit;\r\n    flex-flow: inherit;\r\n    justify-content: inherit;\r\n    align-items: inherit;\r\n    \r\n    #filtro {\r\n        @include v.filtro;\r\n    }\r\n    \r\n}","style.scss":"@use 'variables' as v;\r\n\r\n@mixin splash() {\r\n    height: 100vh;\r\n    display: flex;\r\n    flex-flow: column nowrap;\r\n    justify-content: center;\r\n    align-items: center;\r\n\r\n    img {\r\n        height: 70vh;\r\n        padding-right: 200px\r\n    }\r\n    \r\n    // background\r\n}\r\n\r\n@mixin centro() {\r\n    height: 40vh;\r\n    display: flex;\r\n    margin-left: 10vw;\r\n    margin-right: 10vw;\r\n    margin-top: 4vh;\r\n    margin-bottom: 4vh;\r\n    flex-flow: row nowrap;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    background-color: v.$branco;\r\n    \r\n    .centro-txt {\r\n        display: block;\r\n        width: 40vw;\r\n        color: v.$azul-escuro;\r\n    }\r\n\r\n\r\n    img {\r\n        width: 20%;   \r\n    }\r\n\r\n    p {\r\n        @include v.normal-1;\r\n    }\r\n}\r\n\r\n@mixin metade($reverse: false) {\r\n    display: flex;\r\n    flex-flow: row nowrap;\r\n    height: 100vh;\r\n    width: 100%;\r\n    background-color: v.$bege;\r\n    \r\n    @if $reverse {\r\n        flex-flow: row-reverse nowrap;\r\n    }\r\n\r\n    .metade-txt {\r\n        margin-top: auto;\r\n        margin-bottom: auto;\r\n        padding-left: 10vw;\r\n        padding-right: 10vw;\r\n        width: 50%;\r\n        box-sizing: border-box;\r\n\r\n        h1 {\r\n            @include v.titulo-1;\r\n        }\r\n\r\n        p {\r\n            @include v.normal-1;\r\n        }\r\n    }\r\n\r\n    .metade-img {\r\n        // BACKGROUND IMAGE\r\n        width: 50%;\r\n    }\r\n}\r\n\r\n.centro {\r\n    @include centro;\r\n}\r\n\r\n.splash {\r\n    @include splash;\r\n}\r\n\r\n.metade {\r\n    @include metade;\r\n}\r\n.metade-reverse {\r\n    @include metade(true);\r\n}"},"lineCount":null}},"error":null,"hash":"575d156546c5dd7254d3d7bc0e5f09d1","cacheData":{"env":{}}}